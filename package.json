{
    "name": "roxanne",
    "version": "3.0.0-beta.9",
    "description": "roxanne is an implementation of the Redux pattern using rxjs.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/ophirbushi/roxanne.git"
    },
    "keywords": [
        "rxjs",
        "rx",
        "simple",
        "redux",
        "typescript",
        "state",
        "state-management"
    ],
    "scripts": {
        "docs": "typedoc --out docs ./src --excludeNotExported --exclude \"**/*+(index|.spec|.e2e).ts\"",
        "rm:dist": "rimraf ./dist",
        "rm:docs": "rimraf ./docs",
        "rm": "npm run rm:dist && npm run rm:docs",
        "prebuild": "npm run test && npm run rm && npm run docs",
        "build": "tsc -p .",
        "pretest": "tsc -p ./spec",
        "test": "jasmine",
        "prepublish": "npm run build"
    },
    "author": "Ophir Bushinsky",
    "license": "MIT",
    "peerDependencies": {
        "rxjs": ">=6.3.3"
    },
    "devDependencies": {
        "@types/jasmine": "^2.8.6",
        "jasmine": "^3.1.0",
        "rimraf": "^2.6.3",
        "rxjs": "^6.5.4",
        "typedoc": "^0.17.7",
        "typescript": "^3.9.2"
    }
}
